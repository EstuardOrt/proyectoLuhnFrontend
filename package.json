{
  "name": "proyecto-frontend",
  "version": "1.0.0",
  "description": "Frontend con pruebas unitarias usando Jest",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=package.json"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {},
    "testMatch": ["**/__tests__/**/*.test.js"]
  }
}
